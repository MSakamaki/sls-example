{
  "compilerOptions": {
    "outDir": "./dist",
    "types": [
      "node"
    ],
    "module": "commonjs",
    "target": "es2017",
    "lib": [
      "es2019"
    ],
    "declaration": false,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "functions/app.js",
    "functions/lambda.js",
    "functions/local.js"
  ]
}